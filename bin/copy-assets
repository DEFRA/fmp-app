cp node_modules/jquery/dist/jquery.min.map server/public/assets/javascripts/jquery.min.map
cp node_modules/openlayers/css/ol.css server/public/assets/stylesheets/ol.css
cp node_modules/govuk-elements-sass/node_modules/govuk_frontend_toolkit/javascripts/govuk/show-hide-content.js server/public/assets/javascripts/show-hide-content.js
