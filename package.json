    "@hapi/good-console": "^9.0.1",
    "@hapi/good-squeeze": "^6.0.0",
    "@hapi/h2o2": "^9.1.0",
    "@hapi/hapi": "^20.2.1",
    "@hapi/hoek": "^9.2.1",
    "@hapi/inert": "^6.0.5",
    "@hapi/vision": "^6.1.0",
    "@hapi/wreck": "^17.1.0",
    "@hapi/yar": "^10.1.1",
    "@turf/polygon-smooth": "^6.5.0",
    "@turf/turf": "^6.5.0",
    "accessible-autocomplete": "2.0.4",
    "area-polygon": "^1.0.1",
    "bcrypt": "^5.0.1",
    "classlist-polyfill": "1.2.0",
    "geojson": "0.5.0",
    "good-file": "6.0.1",
    "govuk-frontend": "^4.0.1",
    "handlebars": "^4.7.7",
    "https-proxy-agent": "^5.0.0",
    "joi": "^17.6.0",
    "jquery": "^3.6.0",
    "jsdom": "^20.0.0",
    "moment-timezone": "0.5.35",
    "ngr-to-bng": "0.0.1",
    "node-hapi-airbrake": "0.1.3",
    "node-sass": "^7.0.1",
    "node-schedule": "^2.1.0",
