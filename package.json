    "blipp": "^4.0.1",
    "browserify": "16.2.3",
    "code": "5.2.4",
    "codeclimate-test-reporter": "0.5.1",
    "exorcist": "1.0.1",
    "govuk_template_mustache": "0.24.1",
    "govuk-elements-sass": "3.1.3",
    "hbsfy": "2.8.1",
    "lab": "18.0.0",
    "node-sass": "^4.14.1",
    "standard": "^14.3.1",
    "wisk": "1.0.1"
  },
  "standard": {
    "ignore": [
      "server/public/static/javascripts/ol.js",
      "server/public/static/javascripts/components/maps/live.js",
      "server/public/static/javascripts/components/a11y-dialog.js",
