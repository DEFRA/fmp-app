{
  "name": "fmfp",
  "version": "2.9.0",
  "dataVersion": "2.5.1",
  "description": "Flood map for planning",
  "main": "index.js",
  "engines": {
    "node": ">=18.17.0"
  },
  "scripts": {
    "start": "node index.js",
    "clean": "sh ./bin/clean-public",
    "copy": "npm run copy:gov && npm run copy:assets",
    "copy:gov": "sh ./bin/copy-gov && node ./bin/fix-layout.js",
    "copy:assets": "sh ./bin/copy-assets",
    "build:js": "sh ./bin/build-js",
    "build:css": "sh ./bin/build-css",
    "build": "sh ./bin/build",
    "build-map": "rm dist/* -f && webpack",
    "lint": "standard",
    "eslint": "eslint --cache --ext .js .",
    "unit-test": "lab --verbose -l -c -r console -o stdout -r lcov -o lcov.info",
    "unit-jest": "jest --runInBand --verbose",
    "unit-test-quiet": "lab -l -s",
    "test": "npm run lint && npm run unit-jest",
    "jest": "jest",
    "test-bail": "lab -l --bail",
    "postinstall": "npm run build",
    "kill": "sudo kill -9 $(sudo lsof -t -i:3000)",
    "increment-version": "npm version prerelease --preid=pre -m \"Set Version to %s\"",
    "set-config": "run(){  cp ./config/server.$1.json ./config/server.json; }; run"
  },
  "author": "defra",
  "license": "ISC",
