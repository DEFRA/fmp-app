    "good": "8.1.1",
    "good-console": "7.1.0",
    "good-file": "6.0.1",
    "good-squeeze": "5.1.0",
    "h2o2": "8.1.2",
    "handlebars": "4.0.12",
    "inert": "5.1.2",
    "joi": "14.3.0",
    "jquery": "3.3.1",
    "moment-timezone": "0.5.26",
    "ngr-to-bng": "0.0.1",
    "node-hapi-airbrake": "0.1.3",
    "node-sass": "^4.14.1",
    "openlayers": "4.6.5",
