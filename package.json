{
  "name": "fmp-app",
  "version": "0.0.1",
  "description": "Flood map for planning web application",
  "main": "index.js",
  "engines": {
    "node": "4.x"
  },
  "scripts": {
    "start": "pm2 start config/pm2.json",
    "clean": "bin/clean-public",
    "copy": "npm run copy:gov && npm run copy:assets",
    "copy:gov": "bin/copy-gov && bin/fix-layout.js",
    "copy:assets": "bin/copy-assets",
    "build:js": "bin/build-js",
    "build:js-map": "./bin/build-js-map",
    "build:css": "bin/build-css",
    "build": "bin/build",
    "watch": "bin/watch",
    "lint": "standard",
    "test": "npm run lint && lab -c -r console -o stdout -r lcov -o lcov.info",
    "postinstall": "npm run build"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "boom": "3.1.3",
    "glue": "3.3.0",
    "good": "7.0.0",
    "good-console": "6.1.1",
    "good-file": "6.0.1",
    "good-squeeze": "3.0.1",
    "handlebars": "4.0.5",
    "hapi": "13.4.0",
    "inert": "4.0.0",
    "joi": "8.1.0",
    "jquery": "1.11.3",
    "openlayers": "3.14.0",
    "proj4": "2.3.12",
    "raf": "3.2.0",
    "sprintf-js": "1.0.3",
    "vision": "4.1.0",
    "wreck": "7.0.0"
  },
  "devDependencies": {
    "browserify": "13.0.1",
    "code": "3.0.1",
    "codeclimate-test-reporter": "0.3.3",
    "exorcist": "0.4.0",
    "govuk-elements-sass": "1.1.2",
    "govuk_template_mustache": "0.16.4",
    "hbsfy": "2.7.0",
    "lab": "10.8.2",
    "node-sass": "3.7.0",
    "standard": "7.0.1",
    "wisk": "1.0.0"
  }
}
