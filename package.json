{
  "name": "fmfp",
  "version": "0.3.8",
  "description": "Flood map for planning",
  "main": "index.js",
  "engines": {
    "node": ">12"
  },
  "scripts": {
    "start": "pm2 start config/pm2.json",
    "clean": "bin/clean-public",
    "copy": "npm run copy:gov && npm run copy:assets",
    "copy:gov": "bin/copy-gov && bin/fix-layout.js",
    "copy:assets": "bin/copy-assets",
    "watch": "bin/watch",
    "build:js": "bin/build-js",
    "build:css": "bin/build-css",
    "build": "bin/build",
    "lint": "standard",
    "unit-test": "lab --verbose -c -r console -o stdout -r lcov -o lcov.info",
    "test": "npm run lint && npm run unit-test",
    "postinstall": "npm run build",
    "kill": "sudo kill -9 $(sudo lsof -t -i:3000)"
  },
  "author": "defra",
  "license": "ISC",
