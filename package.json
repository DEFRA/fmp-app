    "node-sass": "^7.0.1",
    "proxyquire": "^2.1.3",
    "standard": "^16.0.4",
    "wisk": "1.0.1"
  },
  "standard": {
    "ignore": [
      "server/public/static/javascripts/ol.js",
      "server/public/static/javascripts/components/maps/live.js",
      "server/public/static/javascripts/components/a11y-dialog.js",
      "server/public/static/javascripts/core.js"
    ]
  }
}
