    "@hapi/inert": "^5.2.2",
    "@hapi/joi": "^16.1.7",
    "@hapi/vision": "^5.5.4",
    "@hapi/wreck": "^16.0.1",
    "@hapi/yar": "^9.2.1",
    "@turf/polygon-smooth": "^5.0.1",
    "@turf/turf": "^5.1.6",
